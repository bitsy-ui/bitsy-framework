{
  "name": "@bitsy-ui/cli",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "bin": {
    "bitsy-ui": "./lib/cmd.js"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsy-ui/bitsy-framework.git"
  },
  "bugs": {
    "url": "https://github.com/bitsy-ui/bitsy-framework/issues"
  },
  "scripts": {
    "build": "rimraf lib && yarn run build:js && yarn run build:ts",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:js": "BABEL_ENV=build babel --root-mode upward src --out-dir lib --extensions .js,.ts,.tsx --no-comments && tsc --project ./tsconfig.json",
    "deploy": "yarn run build && npm unpublish --force --registry http://localhost:4873 @bitsy-ui/cli && npm publish --registry http://localhost:4873"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.10.1",
    "@bitsy-ui/config": "^0.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "commander": "^7.2.0",
    "compression": "^1.7.4",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.21",
    "open": "^8.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.3",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-manifest-plugin": "^3.1.1"
  },
  "devDependencies": {
    "ts-jest": "^25.3.1"
  }
}

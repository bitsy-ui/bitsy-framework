{
  "name": "@bitsy-ui/core",
  "version": "0.3.3",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "babel": "babel",
    "webpack": "webpack",
    "src": "src"
  },
  "files": [
    "lib",
    "babel",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsy-ui/bitsy-framework.git"
  },
  "bugs": {
    "url": "https://github.com/bitsy-ui/bitsy-framework/issues"
  },
  "scripts": {
    "build": "rimraf lib && yarn run build:js && yarn run build:ts",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:js": "BABEL_ENV=build babel --root-mode upward src --out-dir lib --extensions .js,.ts,.tsx --no-comments && tsc --project ./tsconfig.json",
    "deploy": "yarn run build && npm unpublish --force --registry http://localhost:4873 @bitsy-ui/core && npm publish --registry http://localhost:4873"
  },
  "dependencies": {
    "@bitsy-ui/bootstrap": "^0.1.4",
    "@bitsy-ui/config": "^0.0.1",
    "compression-webpack-plugin": "^7.1.2",
    "core-js": "^3.9.1",
    "fastify": "^3.15.1",
    "fastify-compress": "^3.5.0",
    "fastify-cors": "^6.0.1",
    "fastify-static": "^4.2.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.3",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-manifest-plugin": "^3.1.1"
  }
}

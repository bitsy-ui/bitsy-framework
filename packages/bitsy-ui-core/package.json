    "helmet": "^4.4.1",
    "react": "^17.0.1",
    "react-async-ssr": "^0.7.2",
    "react-dom": "^17.0.1",
    "react-ssr-prepass": "^1.4.0",
    "regenerator-runtime": "^0.13.7",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.5.0",
    "@bitsy-ui/bootstrap": "^0.0.1"

    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.3",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^4.5.0"
  }

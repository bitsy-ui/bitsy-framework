{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
    },
    "module": "ES2020",
    "target": "ES2019",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": false,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "checkJs": false,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
